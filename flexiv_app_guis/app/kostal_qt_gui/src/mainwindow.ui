}
#MidContainer{
}
QFrame, QLabel {
}
/* Set settings for checkbox*/
QCheckBox{
    border: 1px solid rgb(40, 87, 128);
    border-radius: 10px;
    background-color: rgb(255, 255, 255);
    padding-left: 150px;
    padding-right: 20px;
}
QCheckBox:indicator{
    width: 32px;
    height: 32px;
}
QCheckBox:hover{
    border: 2px solid rgb(85,170,255);
}
QCheckBox:focus {
}
QCheckBox:indicator::unchecked{
    image: url(:/icons/icons/square.svg);
}
QCheckBox:indicator::checked{
    background: rgb(202, 222, 237);
    image: url(:/icons/icons/check-square.svg);
}
/* Set settings for line edit inputs*/
QLineEdit{
    border: 1px solid rgb(40, 87, 128);
    border-radius: 10px;
    background-color: rgb(255, 255, 255);
    padding-left: 20px;
    padding-right: 20px;
    color: rgb(40, 87, 128);
    font: bold;
    qproperty-alignment: AlignCenter;
}
QLineEdit:hover{
    border: 2px solid rgb(85,170,255);
}
QLineEdit:focus {
    border: 2px solid rgb(85,170,255);
}
/* Set settings for double spinbox inputs*/
QSpinBox, QDoubleSpinBox{
    margin-left: 50px;  /* make room for the arrows */
    margin-right: 50px;  /* make room for the arrows */
    border: 1px solid rgb(40, 87, 128);
    background-color: rgb(255, 255, 255);
    color: rgb(40, 87, 128);
    font: bold;
}
QSpinBox::hover, QDoubleSpinBox::hover{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::focus, QDoubleSpinBox::focus{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::down-button, QDoubleSpinBox::down-button{
    subcontrol-origin: margin;
    subcontrol-position: center left;
    border: 1px solid;
    border-radius: 5px;
    border-color: rgb(46, 103, 156);
    background-color: rgb(202, 222, 237);
    height: 54px;
    width: 54px;
}
QSpinBox::down-arrow, QDoubleSpinBox::down-arrow{
    image: url(:/icons/icons/arrow-down-circle.svg);;
    height: 35px;
    width: 35px;
}
QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::down-button:focus, QDoubleSpinBox::down-button:focus{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::down-button:pressed, QDoubleSpinBox::down-button:pressed{
    background-color: rgb(52, 113, 173);
}
QSpinBox::up-button, QDoubleSpinBox::up-button{
    subcontrol-origin: margin;
    subcontrol-position: center right;
    border: 1px solid;
    border-radius: 5px;
    border-color: rgb(46, 103, 156);
    background-color: rgb(202, 222, 237);
    height: 54px;
    width: 54px;
}
QSpinBox::up-arrow, QDoubleSpinBox::up-arrow{
    image: url(:/icons/icons/arrow-up-circle.svg);
    height: 35px;
    width: 35px;
}
QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::up-button:focus, QDoubleSpinBox::up-button:focus{
    border: 2px solid rgb(85,170,255);
}
QSpinBox::up-button:pressed, QDoubleSpinBox::up-button:pressed{
    background-color: rgb(52, 113, 173);
}
/* Set settings for push buttons*/
QPushButton{
    border: 1px solid;
    border-color: rgb(46, 103, 156);
    border-left: 1px solid;
    border-radius: 10px;
    padding-right: 20px; 
    padding-left: 20px;
    padding-top: 5px;
    padding-bottom: 5px;
    margin: 1px;
    background-color: rgb(202, 222, 237);
    font: bold;
}
QPushButton:hover {
    border: 2px solid rgb(85,170,255);
}
QPushButton:focus {
    border: 2px solid rgb(85,170,255);
}
QPushButton:pressed{
    background-color: rgb(52, 113, 173);
}
QPushButton:disabled {
    color: grey;
    border-color: grey;
    background-color: none;
}</string>
